+  /**
+   * Batch geocode multiple locations efficiently
+   */
+  public Map<String, LocationCoordinates> batchGeocode(List<String> locationNames) {
+    log.info("Batch geocoding {} locations", locationNames.size());
+
+    Map<String, LocationCoordinates> results = new HashMap<>();
+
+    for (String locationName : locationNames) {
+      try {
+        // Rate limiting for OpenStreetMap API
+        if (lastRequestTime > 0) {
+          long timeSinceLastRequest = System.currentTimeMillis() - lastRequestTime;
+          if (timeSinceLastRequest < RATE_LIMIT_DELAY) {
+            Thread.sleep(RATE_LIMIT_DELAY - timeSinceLastRequest);
+          }
+        }
+
+        Optional<LocationCoordinates> coordinates = fetchLocationCoordinates(locationName);
+        coordinates.ifPresent(coords -> results.put(locationName, coords));
+
+        lastRequestTime = System.currentTimeMillis();
+
+      } catch (Exception e) {
+        log.error("Error geocoding location '{}': {}", locationName, e.getMessage());
+      }
+    }
+
+    log.info("Successfully geocoded {}/{} locations", results.size(), locationNames.size());
