+  /**
+   * Enrich a single location with coordinates
+   */
+  private LocationDTO enrichLocationWithCoordinates(LocationDTO location) {
+    double lat = location.latitude();
+    double lon = location.longitude();
+
+    // If coordinates already exist, return as-is
+    if (lat != 0.0 && lon != 0.0) {
+      return location;
+    }
+
+    // Fetch coordinates from OpenStreetMap
+    Optional<LocationCoordinates> coordinates = fetchLocationCoordinates(location.name());
+
+    if (coordinates.isPresent()) {
+      LocationCoordinates coords = coordinates.get();
+      return new LocationDTO(
+          location.id(),
+          location.name(),
+          location.translatedName(),
+          coords.getLatitude(),
+          coords.getLongitude());
+    }
+
+    // Return original if no coordinates found
+    log.warn("No coordinates found for location: {}", location.name());
+    return location;
+  }
