# Perundhu Environment Variables
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=jdbc:postgresql://localhost:5432/perundhu
DATABASE_USER=perundhu
DATABASE_PASSWORD=your_secure_password_here

# Google Cloud Configuration (Production)
GOOGLE_CLOUD_PROJECT=your-project-id
GCS_BUCKET_NAME=your-bucket-name

# OCR Service Configuration
OCR_SERVICE_ENABLED=true
OCR_SERVICE_URL=http://localhost:8081
OCR_SERVICE_TIMEOUT=60

# Spring Profile (dev, test, prod, preprod)
SPRING_PROFILES_ACTIVE=dev
